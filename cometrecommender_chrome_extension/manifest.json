{
  "name": "Recommended Talks",
  "version": "1.0",
  "description": "A browser action with a popup dump of all recommended talks.",
  "permissions": [
    "bookmarks","http://localhost:8080/cometrecommender/"
  ],
  "browser_action": {
      "default_title": "CoMeT Recommended Talks",
      "default_icon": "comet_logo.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
